{
    "name": "st693ava/filament-events-manager",
    "description": "Sistema de gestão de eventos automatizados para Filament v4 com interface em Português",
    "keywords": ["filament", "events", "automation", "laravel", "eloquent", "activity-log"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "st693ava",
            "email": "pdro.saraiva@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "laravel/framework": "^12.0",
        "spatie/laravel-activitylog": "^4.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "orchestra/testbench": "^10.0",
        "orchestra/testbench-dusk": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "pestphp/pest-plugin-faker": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-drift": "^4.0",
        "laravel/dusk": "^8.0",
        "orchestra/workbench": "*",
        "pestphp/pest-plugin-browser": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "St693ava\\FilamentEventsManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "St693ava\\FilamentEventsManager\\EventsManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
